.room-image-container {
  position: relative;
}

.room-image {
  width: 100%;
  height: auto;
  display: block;
}

.favorite-icon {
  position: absolute;
  top: 10px;
  right: 10px;
  background-color: rgba(255, 255, 255, 0.8);
  padding: 5px;
  border-radius: 50%;
  cursor: pointer;
}
.room-card {
  /* Adjust card styles */
  border-radius: 8px;
  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);
  transition: transform 0.3s;
}
.result_find {
  color: red;
}
